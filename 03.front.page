<?xml version="1.0" encoding="UTF-8"?>
<page>
  <background-color>0.0 0.0 0.0</background-color>
  <blocks>
    <!-- Photo -->
    <block type="image" referential-anchor="center" autosize="yes">
      <state id="portrait">
        <position>128px -102px</position>
        <size>758px 680px</size>
      </state>
      <state id="landscape">
        <position>0px -108px</position>
        <size>1014px 480px</size>
      </state>
      <autosizing>aspect-fill</autosizing>
      <image>Front-Card3.jpg</image>
    </block>
    <!-- Border -->
    <block id="borders" type="group" referential-anchor="top-left" anchor="top-left">
    </block>
    <!-- Plate -->
    <block type="image" referential-anchor="bottom-left" anchor="bottom-left">
      <state id="portrait">
        <size>758px 204px</size>
        <position>0px 0px</position>
      </state>
      <state id="landscape">
        <size>1014px 148px</size>
        <position>0px 0px</position>
      </state>
      <background-color>0.0 0.0 0.0</background-color>
